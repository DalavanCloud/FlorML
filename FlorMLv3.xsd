			<xs:element ref="url" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="figureLegend" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="id" type="xs:ID" use="required">
			<xs:annotation>
				<xs:documentation>figure ID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="photo">
						<xs:annotation>
							<xs:documentation>Photograph of any subject.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="lineart">
						<xs:annotation>
							<xs:documentation>Lineart drawing of any subject.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="signature">
						<xs:annotation>
							<xs:documentation>For signatures and handwriting.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="other">
						<xs:annotation>
							<xs:documentation>Any other image that doesn't fit into the other choices. Can also be used for  text elements for which no modern digital equivalent exists.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="lang" type="xs:language">
			<xs:annotation>
				<xs:documentation>language used for accompanying text </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="footnoteStringType" mixed="true">
		<xs:annotation>
			<xs:documentation>footnote string</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="fullName" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="nomenclature" minOccurs="0"/>
			<xs:element ref="feature" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="gathering" type="gatheringType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="references" type="referencesType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>section of literature references</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="b" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="i" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="sub" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>subscript text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="sup" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>superscript text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="tableRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="table" type="tableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>actual table</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="footnoteTypeRef" mixed="true">
		<xs:annotation>
			<xs:documentation>footnote reference</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="num" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="ref" type="xs:IDREF" use="required">
			<xs:annotation>
				<xs:documentation>reference ID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="footnoteType">
		<xs:annotation>
			<xs:documentation>footnote</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="footnoteString" type="footnoteStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>footnote text</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="lang" type="xs:language">
			<xs:annotation>
				<xs:documentation>language of footnote</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="id" type="xs:ID">
			<xs:annotation>
				<xs:documentation>reference ID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="habitatListType">
		<xs:annotation>
			<xs:documentation>list of habitats</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="habitat" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>one habitat</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="headingType" mixed="true">
		<xs:annotation>
			<xs:documentation>heading</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>reference to footnote</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="homonymType" mixed="true">
		<xs:annotation>
			<xs:documentation>homonym</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element ref="fullName" minOccurs="0"/>
			<xs:element ref="name" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="citation" type="citationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="introductionType">
		<xs:choice maxOccurs="unbounded">
			<xs:element name="heading" type="headingType"/>
			<xs:element name="writer" type="writerType" minOccurs="0"/>
			<xs:element name="string" type="stringType"/>
			<xs:element name="references" type="referencesType" minOccurs="0"/>
			<xs:element name="figure" type="figureType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="keyType">
		<xs:annotation>
			<xs:documentation>key</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="keyTitle" minOccurs="0"/>
			<xs:element ref="keynotes" minOccurs="0"/>
			<xs:element ref="couplet" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>one couplet</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="lang" type="xs:language">
			<xs:annotation>
				<xs:documentation>language of key</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="id">
			<xs:annotation>
				<xs:documentation>key ID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="multiAccessKey" type="xs:boolean" default="false"/>
	</xs:complexType>
	<xs:complexType name="listItemType" mixed="true">
		<xs:annotation>
			<xs:documentation>list item</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="fullName" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="distributionLocality" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="footnote" type="footnoteType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="gathering" type="gatheringType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="b" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="i" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="sub" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="sup" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>comment by treatment author</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="references" type="referencesType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="figureRef" type="figureRefType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="type" use="optional">
			<xs:annotation>
				<xs:documentation>some possible list item types</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="locality"/>
					<xs:enumeration value="altitude"/>
					<xs:enumeration value="density"/>
					<xs:enumeration value="surface"/>
					<xs:enumeration value="distance"/>
					<xs:enumeration value="percentage"/>
					<xs:enumeration value="ecology"/>
					<xs:enumeration value="date"/>
					<xs:enumeration value="date_year"/>
					<xs:enumeration value="collector"/>
					<xs:enumeration value="expedition"/>
					<xs:enumeration value="volume"/>
					<xs:enumeration value="part"/>
					<xs:enumeration value="pages"/>
					<xs:enumeration value="plates"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="value"/>
	</xs:complexType>
	<xs:complexType name="metaDataType">
		<xs:annotation>
			<xs:documentation>metadata</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="nomType" mixed="true">
		<xs:annotation>
			<xs:documentation>nomenclature</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="fullName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>unatomized name  (atomizing name is preferred)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="num" minOccurs="0"/>
			<xs:element ref="name" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="citation" type="citationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>name citations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="homonym" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType mixed="true">
					<xs:complexContent>
						<xs:extension base="homonymType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="basionym" type="basionymType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>annotation by treatment author</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="class" use="required">
			<xs:annotation>
				<xs:documentation>use nametype in nameType only</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="accepted"/>
					<xs:enumeration value="synonym"/>
					<xs:enumeration value="nametype"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="fungiMorph">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="anamorph"/>
					<xs:enumeration value="holomorph"/>
					<xs:enumeration value="teleomorph"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="notesType">
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="heading" type="headingType" minOccurs="0"/>
			<xs:element name="writer" type="writerType" minOccurs="0"/>
			<xs:element name="string" type="stringType" maxOccurs="unbounded"/>
			<xs:element name="figure" type="figureType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>for figures referenced to in nomenclaturalNotes</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="orderedListType">
		<xs:annotation>
			<xs:documentation>ordered list</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element ref="li" maxOccurs="unbounded"/>
			<xs:element name="ol" type="orderedListType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ul" type="unorderedListType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="referencesType">
		<xs:annotation>
			<xs:documentation>references</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="heading" type="headingType" minOccurs="0"/>
			<xs:element name="subHeading" type="subheadingType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="writer" type="writerType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>reference to footnote</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="footnote" type="footnoteType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>actual footnote</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="string" type="stringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>any text not covered by other elements</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="refNum" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="reference" type="referenceType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>a single literature reference</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="lang" type="xs:language"/>
		<xs:attribute name="bibliography" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>For when a section of references stands on its own and is not appanded to a previous text section, feature, or treatment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="serialsAbbreviations" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Some publications include lists that show abbreviations of journal titles and their fully expanded equivalent. Setting this attribute to "true"  in conjunction with using reference.refPart.class="pubname" and reference.refPart.class="pubfullname" allows one to mark up such lists.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="referenceType">
		<xs:annotation>
			<xs:documentation>single reference</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="refPart" type="refPartType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="refPartType" mixed="true">
		<xs:annotation>
			<xs:documentation>reference atomization</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="b" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="i" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>annotation by the treatment's author</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="footnote" type="footnoteType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="class" use="required">
			<xs:annotation>
				<xs:documentation>reference part</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="author"/>
					<xs:enumeration value="pubtitle">
						<xs:annotation>
							<xs:documentation>publication title</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="alternatepubtitle">
						<xs:annotation>
							<xs:documentation>eventually present alternative publication title.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="pubname">
						<xs:annotation>
							<xs:documentation>Publication name or publication name abbreviation. If both are given, use "pubfullname" for the entire publication name.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="pubfullname"/>
					<xs:enumeration value="pubtype">
						<xs:annotation>
							<xs:documentation>Possible options: Article, Book, BookSection, CdDvd, Database, Generic, InProceedings, Journal, Map, NomenclaturalReference, Patent, PersonalCommunication, PrintSeries, Proceedings, Report, Thesis, WebPage</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="series"/>
					<xs:enumeration value="volume"/>
					<xs:enumeration value="issue"/>
					<xs:enumeration value="treatmentNumber">
						<xs:annotation>
							<xs:documentation>Some citations refer to a particular treatment number. If this is present and identifiable as certainly being the treatment number, use this option.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="edition"/>
					<xs:enumeration value="editors"/>
					<xs:enumeration value="publisher">
						<xs:annotation>
							<xs:documentation>Name of publisher + eventual location</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="pages"/>
					<xs:enumeration value="details"/>
					<xs:enumeration value="year">
						<xs:annotation>
							<xs:documentation>ONLY for single years. Otherwise, use fullDate.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="fullDate">
						<xs:annotation>
							<xs:documentation>For any citations/references where instead of a single year a specific date or range of years or similar is given.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="namestatus">
						<xs:annotation>
							<xs:documentation>Use this for nomenclatural name statusses that are appended to a citation of reference instead of the name itself.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="notes">
						<xs:annotation>
							<xs:documentation>Any notes or clarifications following a citation or reference.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="status">
						<xs:annotation>
							<xs:documentation>Publication status ONLY.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="url"/>
					<xs:enumeration value="doi">
						<xs:annotation>
							<xs:documentation>digital object identifier</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="stringType" mixed="true">
		<xs:annotation>
			<xs:documentation>text string</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="heading" type="headingType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="subHeading" type="subheadingType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="num" minOccurs="0"/>
			<xs:element ref="fullName" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>unatomized name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gathering" type="gatheringType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="references" type="referencesType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>comment by treatment author</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="distributionLocality" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>distribution locality. Only valid in feature[class="distribution"] context</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="habitat" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>habitat. Only valid in feature[class="habitat"] context</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="figureRef" type="figureRefType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="figure" type="figureType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="footnote" type="footnoteType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="writer" type="writerType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="dates" type="datesType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="tableRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="table" type="tableType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ol" type="orderedListType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>ordered list</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ul" type="unorderedListType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>unordered list</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="b" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="i" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="sup" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="sub" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="toKey" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="class" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="abbreviation"/>
					<xs:enumeration value="bibliography"/>
					<xs:enumeration value="dateOfPublication"/>
					<xs:enumeration value="dedication"/>
					<xs:enumeration value="introduction"/>
					<xs:enumeration value="notes"/>
					<xs:enumeration value="preface"/>
					<xs:enumeration value="quote"/>
					<xs:enumeration value="serials"/>
					<xs:enumeration value="speciesDelimitation"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="lang" type="xs:language">
			<xs:annotation>
				<xs:documentation>language of string</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="subheadingType" mixed="true">
		<xs:annotation>
			<xs:documentation>subheading</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>reference to footnote</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
