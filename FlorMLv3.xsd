			<xs:sequence>
				<xs:element name="nom" type="nomType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addendumCollector">
		<xs:annotation>
			<xs:documentation>to what collector('s biography) is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumFamily">
		<xs:annotation>
			<xs:documentation>to which family does the addendum belong?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumPage">
		<xs:annotation>
			<xs:documentation>to what page is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumTaxon">
		<xs:annotation>
			<xs:documentation>to what taxon is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="addendumVolume">
		<xs:annotation>
			<xs:documentation>to what volume is this an addendum?</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeCollectionTypeStatus">
		<xs:annotation>
			<xs:documentation>alternative collection type status</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeCollector">
		<xs:annotation>
			<xs:documentation>alternative collector</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeFieldNum">
		<xs:annotation>
			<xs:documentation>alternative number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="altitude">
		<xs:annotation>
			<xs:documentation>altitude</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="b">
		<xs:annotation>
			<xs:documentation>text in bold</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="br">
		<xs:annotation>
			<xs:documentation>line break</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="collectionTypeStatus">
		<xs:annotation>
			<xs:documentation>collection type status</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="collection">
		<xs:annotation>
			<xs:documentation>collection</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="collectionTypeStatus" minOccurs="0"/>
				<xs:element ref="alternativeCollectionTypeStatus" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subCollection" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="status">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="epitype"/>
						<xs:enumeration value="holotype"/>
						<xs:enumeration value="iconotype"/>
						<xs:enumeration value="isoepitype"/>
						<xs:enumeration value="isolectotype"/>
						<xs:enumeration value="isoneotype"/>
						<xs:enumeration value="isosyntype"/>
						<xs:enumeration value="isotype"/>
						<xs:enumeration value="lectotype"/>
						<xs:enumeration value="lectoholotype"/>
						<xs:enumeration value="neotype"/>
						<xs:enumeration value="original material"/>
						<xs:enumeration value="paralectotype"/>
						<xs:enumeration value="paraneotype"/>
						<xs:enumeration value="paratype"/>
						<xs:enumeration value="phototype"/>
						<xs:enumeration value="second step lectotype"/>
						<xs:enumeration value="second step neotype"/>
						<xs:enumeration value="syntype"/>
						<xs:enumeration value="uncertain"/>
						<xs:enumeration value="unspecific"/>
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="notSeen" type="xs:boolean" default="false"/>
			<xs:attribute name="unknown" type="xs:boolean" default="false"/>
			<xs:attribute name="destroyed" type="xs:boolean" default="false"/>
			<xs:attribute name="lost" type="xs:boolean" default="false"/>
			<xs:attribute name="notFound" type="xs:boolean" default="false"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="collector">
		<xs:annotation>
			<xs:documentation>collector</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="couplet">
		<xs:annotation>
			<xs:documentation>couplet</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="question" maxOccurs="unbounded"/>
				<xs:element ref="keynotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="num" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="day" type="xs:gDay">
		<xs:annotation>
			<xs:documentation>day date (DD)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="datepub">
		<xs:annotation>
			<xs:documentation>dates of publication</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="string" type="stringType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="dedication">
		<xs:annotation>
			<xs:documentation>for a dedication</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="distributionLocality">
		<xs:annotation>
			<xs:documentation>list item (locality) for distribution</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element name="coordinates" type="coordinatesType" minOccurs="0"/>
				<xs:element ref="distributionLocality" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="world"/>
						<xs:enumeration value="continent"/>
						<xs:enumeration value="continental region"/>
						<xs:enumeration value="country"/>
						<xs:enumeration value="province"/>
						<xs:enumeration value="region"/>
						<xs:enumeration value="district"/>
						<xs:enumeration value="locality"/>
						<xs:enumeration value="state"/>
						<xs:enumeration value="county"/>
						<xs:enumeration value="town"/>
						<xs:enumeration value="tdwg1"/>
						<xs:enumeration value="tdwg2"/>
						<xs:enumeration value="tdwg3"/>
						<xs:enumeration value="tdwg4"/>
						<xs:enumeration value="other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="status">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="cultivated"/>
						<xs:enumeration value="endemic"/>
						<xs:enumeration value="native"/>
						<xs:enumeration value="naturalized"/>
						<xs:enumeration value="introduced"/>
						<xs:enumeration value="absent"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="frequency">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="extensive"/>
						<xs:enumeration value="rare"/>
						<xs:enumeration value="widespread"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="doubtful" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>true if there is some doubt about the occurrence in this area or about the status. May also be indicated by using an ? in the distribution text. Example: "?Sumatra"</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="extra" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>True, if this disbributionLocality has been added additionally to the text, indicating that the distribution text is not part of the original text. This is sometimes necessary if e.g. an area is mentioned only partly within an enumeration or the way how it needs to be parsed is not in accordance the original text. 
Example: West, East and North Africa. If you want to markup West Africa as a separate region this is not possible, because "North" is not a unique area name. Instead you do: North <distribtuionLocality extra="true">North Africa</distribtuionLocality>, South ...</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="feature">
		<xs:annotation>
			<xs:documentation>any feature (e.g. description, distribution, geography, ecology, notes, etc.)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="heading" type="headingType" minOccurs="0"/>
				<xs:element name="writer" type="writerType" minOccurs="0"/>
				<xs:element ref="num" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="char" type="charType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="habitatList" type="habitatListType" minOccurs="0"/>
				<xs:element name="string" type="stringType" minOccurs="0"/>
				<xs:element name="figureRef" type="figureRefType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="figure" type="figureType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="references" type="referencesType" minOccurs="0"/>
			</xs:choice>
			<xs:attribute name="lang" type="xs:language"/>
			<xs:attribute name="isNotes" type="xs:boolean" default="false"/>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="anatomy"/>
						<xs:enumeration value="bark"/>
						<xs:enumeration value="biogeography"/>
						<xs:enumeration value="conservation"/>
						<xs:enumeration value="cultivation"/>
						<xs:enumeration value="cytology"/>
						<xs:enumeration value="description"/>
						<xs:enumeration value="dispersal"/>
						<xs:enumeration value="distribution"/>
						<xs:enumeration value="ecology"/>
						<xs:enumeration value="embryology"/>
						<xs:enumeration value="fossils"/>
						<xs:enumeration value="habitat"/>
						<xs:enumeration value="habitatecology"/>
						<xs:enumeration value="history"/>
						<xs:enumeration value="introducedspecies"/>
						<xs:enumeration value="lifehistory"/>
						<xs:enumeration value="material"/>
						<xs:enumeration value="morphology"/>
						<xs:enumeration value="notes"/>
						<xs:enumeration value="palynology"/>
						<xs:enumeration value="parasitism"/>
						<xs:enumeration value="phytochemo"/>
						<xs:enumeration value="phylogeny"/>
						<xs:enumeration value="pollination"/>
						<xs:enumeration value="specimens"/>
						<xs:enumeration value="taxonomy"/>
						<xs:enumeration value="uses"/>
						<xs:enumeration value="vernacular"/>
						<xs:enumeration value="wood"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="fieldNum">
		<xs:annotation>
			<xs:documentation>field number</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence minOccurs="0">
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="figureLegend">
		<xs:annotation>
			<xs:documentation>figure legend</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element name="gathering" type="gatheringType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fullName" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>footnote reference number</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="footnote" type="footnoteType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="references" type="referencesType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="annotation" type="annotationType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="figureRef" type="figureRefType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="i" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="sub" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="sup" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="b" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="figureTitle">
		<xs:annotation>
			<xs:documentation>figure title</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fullDate" type="xs:string">
		<xs:annotation>
			<xs:documentation>freetext dates for dates that don't fit into the other types</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fullName">
		<xs:annotation>
			<xs:documentation>unatomized name (atomized name is preferred)</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="rank" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="family"/>
						<xs:enumeration value="subfamily"/>
						<xs:enumeration value="tribe"/>
						<xs:enumeration value="genus"/>
						<xs:enumeration value="subgenus"/>
						<xs:enumeration value="section"/>
						<xs:enumeration value="series"/>
						<xs:enumeration value="subsection"/>
						<xs:enumeration value="species"/>
						<xs:enumeration value="species group"/>
						<xs:enumeration value="subspecies"/>
						<xs:enumeration value="variety"/>
						<xs:enumeration value="subvariety"/>
						<xs:enumeration value="forma"/>
						<xs:enumeration value="subforma"/>
						<xs:enumeration value="without rank"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="fullType">
		<xs:annotation>
			<xs:documentation>unatomized type designation (atomization is preferred)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="habitat">
		<xs:annotation>
			<xs:documentation>habitat</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="altitude" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="lifeCyclePeriods" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="extra" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>True, if this habitat information has been added additionally to the text, indicating that the habitat text is not part of the original text. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="homotypes">
		<xs:annotation>
			<xs:documentation>homotypes</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element name="nom" type="nomType" maxOccurs="unbounded"/>
				<xs:element ref="nameType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="specimenType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
