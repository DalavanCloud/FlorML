<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="body">
		<xs:annotation>
			<xs:documentation>body element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="cover" minOccurs="0">
					<xs:annotation>
						<xs:documentation>book cover, includes list of family treatments included</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="edcom" minOccurs="0">
					<xs:annotation>
						<xs:documentation>editorial committee information, includes lists of editorial committee, etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="titlepage">
					<xs:annotation>
						<xs:documentation>title page (first page of volume, usually), includes list of treatments etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="copyright"/>
				<xs:element ref="sectiontitlepage" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="abstract" minOccurs="0"/>
				<xs:element ref="contents" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="preface" minOccurs="0"/>
				<xs:element ref="dedication" minOccurs="0">
					<xs:annotation>
						<xs:documentation>dedication page and dedicated person biography</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="abbr" minOccurs="0">
					<xs:annotation>
						<xs:documentation>abbreviation section, includes abbreviation list(s)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="bibliography" minOccurs="0"/>
				<xs:element ref="serials" minOccurs="0"/>
				<xs:element ref="specdelim" minOccurs="0"/>
				<xs:element ref="cyclopedia" minOccurs="0"/>
				<xs:element ref="introduction" minOccurs="0"/>
				<xs:element ref="generalkey" minOccurs="0"/>
				<xs:element ref="datepub" minOccurs="0"/>
				<xs:element ref="taxon" maxOccurs="unbounded"/>
				<xs:element ref="index" minOccurs="0"/>
				<xs:element ref="addenda" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="table">
		<xs:annotation>
			<xs:documentation>table</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tr" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="tr">
		<xs:annotation>
			<xs:documentation>table row</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="th" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="td" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="th">
		<xs:annotation>
			<xs:documentation>table header</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="td">
		<xs:annotation>
			<xs:documentation>table cell data</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="ol">
		<xs:annotation>
			<xs:documentation>ordered list</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="li" maxOccurs="unbounded"/>
				<xs:element ref="ol" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ul" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="ul">
		<xs:annotation>
			<xs:documentation>unordered list</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="li" maxOccurs="unbounded"/>
				<xs:element ref="ul" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ol" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="li">
		<xs:annotation>
			<xs:documentation>list item</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="br">
		<xs:annotation>
			<xs:documentation>line break</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="superscript">
		<xs:annotation>
			<xs:documentation>superscript</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="subscript">
		<xs:annotation>
			<xs:documentation>subscript</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bold">
		<xs:annotation>
			<xs:documentation>text in bold</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="italics">
		<xs:annotation>
			<xs:documentation>text in italics</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="heading">
		<xs:annotation>
			<xs:documentation>heading</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice>
				<xs:element ref="writer" minOccurs="0"/>
				<xs:element ref="footnotes" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="subheading">
		<xs:annotation>
			<xs:documentation>subheading</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice>
				<xs:element ref="writer" minOccurs="0"/>
				<xs:element ref="bold" minOccurs="0"/>
				<xs:element ref="italics" minOccurs="0"/>
				<xs:element ref="footnotes" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="ann">
		<xs:annotation>
			<xs:documentation>annotations</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="name" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="nom" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="citation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="year">
		<xs:annotation>
			<xs:documentation>year (for cover, title page, etc.)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="title">
		<xs:annotation>
			<xs:documentation>flora series title</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="subseries">
		<xs:annotation>
			<xs:documentation>flora subseries</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="string">
		<xs:annotation>
			<xs:documentation>text string</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="subheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="volume" minOccurs="0"/>
				<xs:element ref="volpage" minOccurs="0"/>
				<xs:element ref="year" minOccurs="0"/>
				<xs:element ref="quotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ol" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ul" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="table" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ann" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="signature" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="class" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="edcom"/>
						<xs:enumeration value="copyright"/>
						<xs:enumeration value="titlepage"/>
						<xs:enumeration value="abstract"/>
						<xs:enumeration value="sectiontitlepage"/>
						<xs:enumeration value="preface"/>
						<xs:enumeration value="dedication"/>
						<xs:enumeration value="abbr"/>
						<xs:enumeration value="biblio"/>
						<xs:enumeration value="serials"/>
						<xs:enumeration value="introduction"/>
						<xs:enumeration value="genkey"/>
						<xs:enumeration value="datepub"/>
						<xs:enumeration value="specdelim"/>
						<xs:enumeration value="index"/>
						<xs:enumeration value="notes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="feature">
		<xs:annotation>
			<xs:documentation>other features, taxonomy, dispersal, etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading" minOccurs="0"/>
				<xs:element ref="string" minOccurs="0"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="anatomy"/>
						<xs:enumeration value="biogeography"/>
						<xs:enumeration value="conservation"/>
						<xs:enumeration value="cytology"/>
						<xs:enumeration value="dispersal"/>
						<xs:enumeration value="fossils"/>
						<xs:enumeration value="introducedspecies"/>
						<xs:enumeration value="lifehistory"/>
						<xs:enumeration value="morphology"/>
						<xs:enumeration value="palynology"/>
						<xs:enumeration value="phytochemo"/>
						<xs:enumeration value="pollination"/>
						<xs:enumeration value="taxonomy"/>
						<xs:enumeration value="uses"/>
						<xs:enumeration value="vernacular"/>
						<xs:enumeration value="..."/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="cover">
		<xs:annotation>
			<xs:documentation>book cover</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="title"/>
				<xs:element ref="volume"/>
				<xs:element ref="year"/>
				<xs:element ref="ul"/>
				<xs:element ref="figures" minOccurs="0"/>
				<xs:element ref="subseries" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="edcom">
		<xs:annotation>
			<xs:documentation>editorial committee information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="heading" maxOccurs="unbounded"/>
				<xs:element ref="ul" maxOccurs="unbounded"/>
				<xs:element ref="string"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="titlepage">
		<xs:annotation>
			<xs:documentation>title page (first page of volume, usually)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="title"/>
				<xs:element ref="heading" minOccurs="0"/>
				<xs:element ref="string" minOccurs="0"/>
				<xs:element ref="subseries"/>
				<xs:element ref="figures" minOccurs="0"/>
				<xs:element ref="volume"/>
				<xs:element ref="year"/>
				<xs:element ref="ul" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="copyright">
		<xs:annotation>
			<xs:documentation>copyright notice</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="isbn" minOccurs="0"/>
				<xs:element ref="string"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="isbn">
		<xs:annotation>
			<xs:documentation>ISBN number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sectiontitlepage">
		<xs:annotation>
			<xs:documentation>Pages  that split a volume in several parts that don't belong to any other particular piece of text</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="string" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="writer">
		<xs:annotation>
			<xs:documentation>writer</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="abstract">
		<xs:annotation>
			<xs:documentation>abstract</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="heading" maxOccurs="unbounded"/>
				<xs:element ref="string" maxOccurs="unbounded"/>
				<xs:element ref="isbn" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="contents">
		<xs:annotation>
			<xs:documentation>contents</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="contentsheading" minOccurs="0"/>
				<xs:element ref="contentssubheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="table" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="general"/>
						<xs:enumeration value="section"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="contentsheading">
		<xs:annotation>
			<xs:documentation>contents heading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="contentssubheading">
		<xs:annotation>
			<xs:documentation>contents subheading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="taxontitle">
		<xs:annotation>
			<xs:documentation>'title' of taxon (e.g. family name, species, etc.)</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="writer" minOccurs="0"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="class" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="family"/>
						<xs:enumeration value="subfamily"/>
						<xs:enumeration value="tribus"/>
						<xs:enumeration value="genus"/>
						<xs:enumeration value="subgenus"/>
						<xs:enumeration value="species"/>
						<xs:enumeration value="subspecies"/>
						<xs:enumeration value="variety"/>
						<xs:enumeration value="subvariety"/>
						<xs:enumeration value="forma"/>
						<xs:enumeration value="subforma"/>
						<xs:enumeration value="hybrid"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="preface">
		<xs:annotation>
			<xs:documentation>preface</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading"/>
				<xs:element ref="string"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="dedication">
		<xs:annotation>
			<xs:documentation>dedication page</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading"/>
				<xs:element ref="string" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0"/>
				<xs:element ref="figures" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="abbr">
		<xs:annotation>
			<xs:documentation>abbreviation section</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading"/>
				<xs:element ref="string" minOccurs="0"/>
				<xs:element ref="ul"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bibliography">
		<xs:annotation>
			<xs:documentation>bibliography sections</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading"/>
				<xs:element ref="contents" minOccurs="0"/>
				<xs:element ref="string" minOccurs="0"/>
				<xs:element ref="references" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="serials">
		<xs:annotation>
			<xs:documentation>serial abbreviations</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading"/>
				<xs:element ref="string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="specdelim">
		<xs:annotation>
			<xs:documentation>species delimitation etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading"/>
				<xs:element ref="contents" minOccurs="0"/>
				<xs:element ref="string"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="index" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="cyclopedia">
		<xs:annotation>
			<xs:documentation>cyclopedia of collectors</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading"/>
				<xs:element ref="abbr" minOccurs="0"/>
				<xs:element ref="cyclotext" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="index" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="addenda" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="cyclotext">
		<xs:annotation>
			<xs:documentation>cyclopedia text</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="quotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="contents" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="signature" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="table" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ol" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ul" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="cyclochronology" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="cyclobio" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ann" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="cyclochronology">
		<xs:annotation>
			<xs:documentation>cyclopedia chronology of collections</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="collector" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="cycloyear" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>cyclopedia collection year</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
