			<xs:documentation>publication</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="metaData"/>
				<xs:element name="biographies" type="bibliographyType" minOccurs="0"/>
				<xs:element name="references" type="referencesType" minOccurs="0"/>
				<xs:element name="textSection" type="textSectionType" minOccurs="0"/>
				<xs:element ref="treatment" maxOccurs="unbounded"/>
				<xs:element name="addenda" type="addendaType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="lang" type="xs:language" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="defaultMediaUrl">
		<xs:annotation>
			<xs:documentation>base URL for illustrations</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="metaData">
		<xs:annotation>
			<xs:documentation>metadata</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="metaDataType">
					<xs:choice maxOccurs="unbounded">
						<xs:any namespace="http://www.loc.gov/standards/mods/v3/mods-3-3.xsd" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="defaultMediaUrl"/>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="acceptedName">
