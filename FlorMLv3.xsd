	<xs:complexType name="citationType">
		<xs:annotation>
			<xs:documentation>citation (in nomenclature)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="refPart" type="refPartType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>citation reference parts</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" use="required">
			<xs:annotation>
				<xs:documentation>only use "type" in specimenType or nameType</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="publication"/>
					<xs:enumeration value="usage"/>
					<xs:enumeration value="type">
						<xs:annotation>
							<xs:documentation>Only for type-related citations!</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
