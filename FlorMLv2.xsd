<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="body">
		<xs:annotation>
			<xs:documentation>body element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="cover"/>
				<xs:element ref="edcom" minOccurs="0"/>
				<xs:element ref="titlepage"/>
				<xs:element ref="copyright"/>
				<xs:element ref="sectionheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="abstract" minOccurs="0"/>
				<xs:element ref="contents" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="preface" minOccurs="0"/>
				<xs:element ref="dedication" minOccurs="0"/>
				<xs:element ref="biography" minOccurs="0"/>
				<xs:element ref="abbr" minOccurs="0"/>
				<xs:element ref="bibliography" minOccurs="0"/>
				<xs:element ref="serials" minOccurs="0"/>
				<xs:element ref="specdelim" minOccurs="0"/>
				<xs:element ref="cyclopedia" minOccurs="0"/>
				<xs:element ref="introduction" minOccurs="0"/>
				<xs:element ref="generalkey" minOccurs="0"/>
				<xs:element ref="datepub" minOccurs="0"/>
				<xs:element ref="taxon"/>
				<xs:element ref="index" minOccurs="0"/>
				<xs:element ref="addenda" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="heading">
		<xs:annotation>
			<xs:documentation>heading</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="writer" minOccurs="0"/>
				<xs:element ref="footnotes" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="subheading">
		<xs:annotation>
			<xs:documentation>subheading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="cover">
		<xs:annotation>
			<xs:documentation>book cover</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="covertitle"/>
				<xs:element ref="covervol"/>
				<xs:element ref="coveryear"/>
				<xs:element ref="covertreatments"/>
				<xs:element ref="coverfig" minOccurs="0"/>
				<xs:element ref="coverseries"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="covertitle">
		<xs:annotation>
			<xs:documentation>series title on cover</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="covervol">
		<xs:annotation>
			<xs:documentation>volume number on cover</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="coveryear">
		<xs:annotation>
			<xs:documentation>publication year on cover</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="covertreatments">
		<xs:annotation>
			<xs:documentation>list of treatments on cover</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="coverfig">
		<xs:annotation>
			<xs:documentation>figure on cover</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="illustrationtype"/>
				<xs:element ref="url"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="coverseries">
		<xs:annotation>
			<xs:documentation>subseries on cover</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="edcom">
		<xs:annotation>
			<xs:documentation>editorial committee information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="coverfiglegend"/>
				<xs:element ref="edcomheading" maxOccurs="unbounded"/>
				<xs:element ref="edcomlist" maxOccurs="unbounded"/>
				<xs:element ref="edcominfoheading" maxOccurs="unbounded"/>
				<xs:element ref="edcominfotext"/>
				<xs:element ref="edcompubdep"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="coverfiglegend">
		<xs:annotation>
			<xs:documentation>legend for cover figure</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="italics" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="edcomheading">
		<xs:annotation>
			<xs:documentation>headings on editorial committee information page</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="edcomlist">
		<xs:annotation>
			<xs:documentation>lists on editorial committee page</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="edcominfoheading">
		<xs:annotation>
			<xs:documentation>scientific information heading on edcom page</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="edcominfotext">
		<xs:annotation>
			<xs:documentation>scientific information paragraph on edcom page</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="edcompubdep">
		<xs:annotation>
			<xs:documentation>publications department information on edcom page</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="edcomfig" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="edcomfig">
		<xs:annotation>
			<xs:documentation>figure on edcom page</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="illustrationtype"/>
				<xs:element ref="url"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="titlepage">
		<xs:annotation>
			<xs:documentation>title page (first page of volume, usually)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="tpcountryoffic" minOccurs="0"/>
				<xs:element ref="tpcountry" minOccurs="0"/>
				<xs:element ref="tpgov" minOccurs="0"/>
				<xs:element ref="tptitle"/>
				<xs:element ref="tpheading" minOccurs="0"/>
				<xs:element ref="tppar" minOccurs="0"/>
				<xs:element ref="tpseries"/>
				<xs:element ref="tpfig" minOccurs="0"/>
				<xs:element ref="tpvol"/>
				<xs:element ref="tpeditor" minOccurs="0"/>
				<xs:element ref="tppublish" minOccurs="0"/>
				<xs:element ref="tpyear"/>
				<xs:element ref="tptreatment" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="tptitle">
		<xs:annotation>
			<xs:documentation>title page title</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpseries">
		<xs:annotation>
			<xs:documentation>title page series</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpvol">
		<xs:annotation>
			<xs:documentation>title page volume</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpyear">
		<xs:annotation>
			<xs:documentation>title page year</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tptreatment">
		<xs:annotation>
			<xs:documentation>title page treatment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="tptreatmentwriter"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="tptreatmentwriter">
		<xs:annotation>
			<xs:documentation>title page treatmentwriter</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpcountryoffic">
		<xs:annotation>
			<xs:documentation>title page country official name</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpcountry">
		<xs:annotation>
			<xs:documentation>title page country english</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpgov">
		<xs:annotation>
			<xs:documentation>title page government department</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpheading">
		<xs:annotation>
			<xs:documentation>title page heading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tppar">
		<xs:annotation>
			<xs:documentation>title page paragraphs</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tppublish">
		<xs:annotation>
			<xs:documentation>title page publisher</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tpfig">
		<xs:annotation>
			<xs:documentation>title page figure</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="illustrationtype"/>
				<xs:element ref="url"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="tpeditor">
		<xs:annotation>
			<xs:documentation>title page editor</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="copyright">
		<xs:annotation>
			<xs:documentation>copyright notice</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="copyisbn" minOccurs="0"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="copyisbn">
		<xs:annotation>
			<xs:documentation>ISBN number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sectionheading">
		<xs:annotation>
			<xs:documentation>headings that split a volume in several parts that don't belong to any particular piece of text</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="writer" minOccurs="0"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="writer">
		<xs:annotation>
			<xs:documentation>writer</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="abstract">
		<xs:annotation>
			<xs:documentation>abstract</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="abstractheading" maxOccurs="unbounded"/>
				<xs:element ref="abstractstring" maxOccurs="unbounded"/>
				<xs:element ref="abstractisbn" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="abstractheading">
		<xs:annotation>
			<xs:documentation>abstract heading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="abstractstring">
		<xs:annotation>
			<xs:documentation>abstract text</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="abstractsubheading" maxOccurs="unbounded"/>
				<xs:element ref="bold" maxOccurs="unbounded"/>
				<xs:element ref="italics" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="abstractisbn">
		<xs:annotation>
			<xs:documentation>abstract isbn</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="abstractsubheading">
		<xs:annotation>
			<xs:documentation>abstract subheading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="contents">
		<xs:annotation>
			<xs:documentation>contents</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="contentsheading" minOccurs="0"/>
				<xs:element ref="contentssubheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="table" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="general"/>
						<xs:enumeration value="section"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="contentsheading">
		<xs:annotation>
			<xs:documentation>contents heading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="contentssubheading">
		<xs:annotation>
			<xs:documentation>contents subheading</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="treatmenttitle">
		<xs:annotation>
			<xs:documentation>title of treatment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="writer"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="class" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="family"/>
						<xs:enumeration value="subfamily"/>
						<xs:enumeration value="tribus"/>
						<xs:enumeration value="genus"/>
						<xs:enumeration value="subgenus"/>
						<xs:enumeration value="species"/>
						<xs:enumeration value="subspecies"/>
						<xs:enumeration value="variety"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="preface">
		<xs:annotation>
			<xs:documentation>preface</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading"/>
				<xs:element ref="prefacestring"/>
				<xs:element ref="references" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="prefacestring">
		<xs:annotation>
			<xs:documentation>preface text string</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="subheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="quotes" minOccurs="0"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="signature" minOccurs="0"/>
				<xs:element ref="date" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="dedication">
		<xs:annotation>
			<xs:documentation>dedication page</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dedicationfig" minOccurs="0"/>
				<xs:element ref="dedicationstring"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="dedicationstring">
		<xs:annotation>
			<xs:documentation>dedication text string</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="dedicationfig">
		<xs:annotation>
			<xs:documentation>dedication picture of person</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="illustrationtype"/>
				<xs:element ref="url"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="biography">
		<xs:annotation>
			<xs:documentation>dedicated person biography</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading"/>
				<xs:element ref="biographystring"/>
				<xs:element ref="references" minOccurs="0"/>
				<xs:element ref="figures" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="biographystring">
		<xs:annotation>
			<xs:documentation>dedicated person biography text string</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="quotes" minOccurs="0"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="signature" minOccurs="0"/>
				<xs:element ref="date" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="abbr">
		<xs:annotation>
			<xs:documentation>abbreviation section</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading"/>
				<xs:element ref="abbrstring"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="abbrstring">
		<xs:annotation>
			<xs:documentation>abbreviation section text</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ul"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bibliography">
		<xs:annotation>
			<xs:documentation>bibliography sections</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading"/>
				<xs:element ref="contents" minOccurs="0"/>
				<xs:element ref="bibliostring" minOccurs="0"/>
				<xs:element ref="references" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bibliostring">
		<xs:annotation>
			<xs:documentation>bibliography text string</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="subheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="serials">
		<xs:annotation>
			<xs:documentation>serial abbreviations</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading"/>
				<xs:element ref="serialstring"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="serialstring">
		<xs:annotation>
			<xs:documentation>serial abbreviations text string</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="subheading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ul" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bold" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="italics" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="subscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="superscript" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="footnotes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="signature" minOccurs="0"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="specdelim">
		<xs:annotation>
			<xs:documentation>species delimitation etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="heading"/>
				<xs:element ref="contents" minOccurs="0"/>
				<xs:element ref="sdstring"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="index" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="sdstring">
		<xs:annotation>
			<xs:documentation>species delimitation text string</xs:documentation>
		</xs:annotation>
